{
  "mcpServers": {
    "offshore-leaks": {
      "command": "python",
      "args": [
        "-m", "offshore_leaks_mcp.mcp_server"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "your_password_here",
        "NEO4J_DATABASE": "offshoreleaks"
      }
    }
  }
}
